import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      int [] array  = {1,2,3,4,5};
      int [] arr = new int [array.length];
      arr[0] = array[array.length - 1];
      for(int i = 0;i<array.length - 1;i++){
          arr[i+1] = array[i];
      }

      System.out.println(Arrays.toString(arr));
    }
}


[5,1,2,3,4]
